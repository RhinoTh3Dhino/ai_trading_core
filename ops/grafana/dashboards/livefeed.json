{
  "uid": "livefeed-ops",
  "title": "Live Feed – Ops",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "time": { "from": "now-6h", "to": "now" },
  "refresh": "10s",
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "links": [],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Transport p99 (ms) pr. venue",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, venue) (rate(feed_transport_latency_ms_bucket{venue=~\"$venue\", symbol=~\"$symbol\"}[5m])))",
          "legendFormat": "{{venue}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "Bar close lag (ms) – max pr. venue/symbol",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "max": 60000
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "orientation": "auto"
      },
      "targets": [
        {
          "expr": "max by (venue, symbol) (feed_bar_close_lag_ms{venue=~\"$venue\", symbol=~\"$symbol\"})",
          "legendFormat": "{{venue}} {{symbol}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Bars/sek (global)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "hidden", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum(rate(feed_bars_total{venue=~\"$venue\", symbol=~\"$symbol\"}[1m]))",
          "legendFormat": "bars/s",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Reconnects (seneste 5m) pr. venue",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "bars", "lineWidth": 1 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum by (venue) (increase(feed_reconnects_total{venue=~\"$venue\"}[5m]))",
          "legendFormat": "{{venue}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Feature latency p95/p99 (ms)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, feature) (rate(feature_compute_ms_bucket{symbol=~\"$symbol\"}[5m])))",
          "legendFormat": "p95 {{feature}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, feature) (rate(feature_compute_ms_bucket{symbol=~\"$symbol\"}[5m])))",
          "legendFormat": "p99 {{feature}}",
          "refId": "B"
        }
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Queue depth (live) – max",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 17 },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value"
      },
      "targets": [
        {
          "expr": "max by (queue_name) (feed_queue_depth{queue_name=\"live\"})",
          "legendFormat": "{{queue_name}}",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "venue",
        "label": "Venue",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(feed_bars_total, venue)",
        "refresh": 2,
        "sort": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": { "selected": true, "text": ["All"], "value": ".*" }
      },
      {
        "name": "symbol",
        "label": "Symbol",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(feed_bars_total{venue=~\"$venue\"}, symbol)",
        "refresh": 2,
        "sort": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": { "selected": true, "text": ["All"], "value": ".*" }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
