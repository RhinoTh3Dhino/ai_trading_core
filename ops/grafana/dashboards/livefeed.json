{
  "uid": "livefeed-ops",
  "title": "Live Feed – Ops",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "editable": true,
  "time": { "from": "now-6h", "to": "now" },
  "refresh": "10s",
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "links": [],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Transport p99 (ms) pr. venue",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 },
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "red", "value": 500 } ] }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "max by (venue) (feed_transport_latency_ms:p99{venue=~\"$venue\", symbol=~\"$symbol\"})",
          "legendFormat": "{{venue}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "Bar close lag (ms) – max pr. venue/symbol (5m)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "max": 60000,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 1500 },
              { "color": "red", "value": 3000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "orientation": "auto"
      },
      "targets": [
        {
          "expr": "max by (venue, symbol) (feed_bar_close_lag_ms:max5m{venue=~\"$venue\", symbol=~\"$symbol\"})",
          "legendFormat": "{{venue}} {{symbol}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Bars/sek (global)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "hidden", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "live:bars_per_sec",
          "legendFormat": "bars/s",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Reconnects (seneste 5m) pr. venue",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "bars", "lineWidth": 1 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "expr": "sum by (venue) (feed_reconnects:inc5m{venue=~\"$venue\"})",
          "legendFormat": "{{venue}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Feature latency p95/p99 (ms)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "lineInterpolation": "smooth", "lineWidth": 2 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "right" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "max by (feature) (feature_compute_ms:p95)",
          "legendFormat": "p95 {{feature}}",
          "refId": "A"
        },
        {
          "expr": "max by (feature) (feature_compute_ms:p99)",
          "legendFormat": "p99 {{feature}}",
          "refId": "B"
        }
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Queue depth (max/5m)",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 17 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value"
      },
      "targets": [
        {
          "expr": "max by (queue_name) (live:queue_depth_max)",
          "legendFormat": "{{queue_name}}",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "venue",
        "label": "Venue",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(feed_bars_total, venue)",
        "refresh": 2,
        "sort": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": { "selected": true, "text": ["All"], "value": ".*" }
      },
      {
        "name": "symbol",
        "label": "Symbol",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(feed_bars_total{venue=~\"$venue\"}, symbol)",
        "refresh": 2,
        "sort": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": { "selected": true, "text": ["All"], "value": ".*" }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
